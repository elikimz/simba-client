{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/service-worker\\.js", "dest": "/service-worker.js" },
    { "src": "/manifest\\.json", "dest": "/manifest.json" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/icons/(.*)", "dest": "/icons/$1" },


    { "src": "/(.*\\..*)", "dest": "/$1" },

    
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}